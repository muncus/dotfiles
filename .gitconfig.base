[user]
    name = "Marc Dougherty"
    email = "muncus@gmail.com"

[alias]
    ci = commit
    co = checkout
    cia = commit -a
    revert-pending = checkout #reverts a pending change, not checked in.
    rollback-last = revert HEAD^ # un-do last commit.
    #svn aliases.
    svnup = svn rebase
    svnpush = svn dcommit
    current-branch = !sh -c 'git branch | grep \"*\" | cut -d \" \" -f 2' -

[core]
    excludesfile = ~/.gitignore

[push]
    default = simple
